{
    "name": "contractive_transfer",
    "net": {
        "type": "transfer",
        "k": 3,
        "n_ch_in": 384,
        "n_ch_out": 128,
        "n_ch_guidance": 0,
        "n_ch_downsampler": 64,
        "feat_weight": -1,
        "patch_size": 14,
        "padding_mode": "circular"
    },
    "transforms": {
        "flip_h_prob": 0.5,
        "flip_v_prob": 0.5,
        "angles_deg": [
            0,
            0,
            0,
            0,
            90,
            180,
            270
        ],
        "shift_dirs": [],
        "shift_dists": [],
        "norm": false
    },
    "training": {
        "loss": "smooth_l1",
        "optim": "adamw",
        "lr": 5e-4,
        "batch_size": 32,
        "n_epochs": 5000,
        "save_per": 10,
        "weights_init": "ones"
    }
}