{
  "name": "upsampler_FU_ch32",
  "net": {
    "name": "upsampler_FU_ch32",
    "k": 3,
    "n_ch_in": 32,
    "n_ch_out": 32,
    "n_ch_guidance": 3,
    "n_ch_downsampler": 32,
    "feat_weight": -1,
    "patch_size": 14,
    "padding_mode": "replicate"
  },
  "transforms": {
    "flip_h_prob": 0.5,
    "flip_v_prob": 0.5,
    "angles_deg": [0, 0, 0, 0, 90, 180, 270],
    "shift_dirs": [],
    "shift_dists": [],
    "norm": true
  },
  "training": {
    "loss": "smooth_l1",
    "optim": "adamw",
    "lr": 1e-4,
    "batch_size": 32,
    "n_epochs": 5000,
    "save_per": 10,
    "weights_init": "uniform"
  }
}
